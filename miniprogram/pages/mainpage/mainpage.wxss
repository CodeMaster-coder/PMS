.line{
  display: flex;
  flex-direction: row;
  width: 96.5%;
  border: 5rpx solid #87ceeb;
  border-radius: 30rpx;
  height: 120rpx;
  font-size: 50rpx;
  margin: 10rpx;
}
.left{
  display: flex;
  width: 50%;
  line-height: 120rpx;
  /* text-align: center; */
  flex-direction: row;
  /* justify-content: center; */
  align-items: center;
  margin-left: 100rpx;
}
.redDot{
  background-color: red;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50rpx;
}
.yellowDot{
  background-color: yellow;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50rpx;
  margin-left: 30rpx;
}
.greenDot{
  background-color: green;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50rpx;
  margin-left: 30rpx;
}
.container{
  display: flex;
  font-size: 25rpx;
  flex-direction: row;
  justify-content: flex-end;
  margin-bottom: 20rpx;
}
.red{
  background-color: red;
  width: 30rpx;
  height: 30rpx;
  border-radius: 30rpx;
  margin-right: 10rpx;
}
.yellow{
  background-color: yellow;
  width: 30rpx;
  height: 30rpx;
  border-radius: 30rpx;
  margin-right: 10rpx;
}
.green{
  background-color: green;
  width: 30rpx;
  height: 30rpx;
  border-radius: 30rpx;
  margin-right: 10rpx;
}
.title{
  margin-right: 20rpx;
}
.right{
  display: flex;
  width: 50%;
  line-height: 120rpx;
  /* text-align: center; */
