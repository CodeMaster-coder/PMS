      故障查询
    </view>
  </view>
  <view class="naviBox" bindtap="naviToMatainceShow">
    <view class="naviImage">
      <image src="../images/matainceShow.png"></image>
    </view>
    <view class="naviText">
      维保查询
    </view>
  </view>
</view>
<view class="naviLine1">
 
  
  <view class="naviBox1" bindtap="naviToAuth">
