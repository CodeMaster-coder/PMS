.page{
  width: 100%;
  height: 10000rpx;
  display: flex;
  justify-content: center;
  /* align-items: center; */
  background-color: #dcdcdc;
}
.mainbox{
  width: 500rpx;
  height: 400rpx;
  border-radius: 30rpx;
  background-color: white;
  margin-top: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.nameline{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 10rpx;
}
.title{
  width: 20%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  color: #A9A9A9;
}
.nameInputBox{
  width: 50%;
  border:4rpx solid #3ba8f2;
  border-radius: 10rpx;
  margin-left: 10rpx;
  /* text-align: center; */
}
.registerline{
  display: flex;
  justify-content: center;
  margin-top: 30rpx;
  height: 65rpx;
}
.registerBotton{
  width: 250rpx;
  line-height: 65rpx;
  background-color: #3ba8f2;
  text-align: center;
  border-radius: 40rpx;
}
.typeSelect{
  width:346rpx;
  height: 45rpx;
  color: #808080;
}
